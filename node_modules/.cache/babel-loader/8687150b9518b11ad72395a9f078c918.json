{"ast":null,"code":"import SidebarLayoutVue from \"@/layouts/SidebarLayout.vue\";\nexport default {\n  name: \"FormList\",\n  components: {\n    SidebarLayoutVue\n  },\n  props: {\n    title: String,\n    routerInsert: \"\",\n    routerUpdate: \"\",\n    routerDelete: \"\",\n    tableHeader: [],\n    tableItems: [],\n    itemDelete: null,\n    fieldSearch: \"\"\n  },\n\n  data() {\n    return {\n      pageCount: 0,\n      pageNumber: 0,\n      searchlabel: \"Pesquisar\"\n    };\n  },\n\n  methods: {\n    onInsertItem() {\n      this.$router.push('/' + this.routerInsert);\n    },\n\n    onEditItem(item) {\n      this.$router.push('/' + this.routerUpdate);\n    },\n\n    onDeleteItem() {\n      this.$router.push('/' + this.routerInsert);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoHA;AAEA;EACAA,gBADA;EAEAC;IACAC;EADA,CAFA;EAKAC;IACAC,aADA;IAEAC,gBAFA;IAGAC,gBAHA;IAIAC,gBAJA;IAKAC,eALA;IAMAC,cANA;IAOAC,gBAPA;IAQAC;EARA,CALA;;EAeAC;IACA;MACAC,YADA;MAEAC,aAFA;MAGAC;IAHA;EAKA,CArBA;;EAsBAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA;IACA,CANA;;IAOAC;MACA;IACA;;EATA;AAtBA","names":["name","components","SidebarLayoutVue","props","title","routerInsert","routerUpdate","routerDelete","tableHeader","tableItems","itemDelete","fieldSearch","data","pageCount","pageNumber","searchlabel","methods","onInsertItem","onEditItem","onDeleteItem"],"sourceRoot":"src/components","sources":["FormList.vue"],"sourcesContent":["<template>\r\n  <sidebar-layout-vue>\r\n    <v-container class=\"fill-height flex-column\">\r\n        <v-card :width=\"800\"\r\n          height=\"100%\"\r\n          max-height=\"480\"\r\n          class=\"mx-auto\"\r\n          outilined\r\n          elevation=\"3\">\r\n          <v-container>\r\n            <!--CABEÇALHO-->\r\n            <v-row dense>\r\n              <v-col cols=\"11\">\r\n                <v-toolbar flat\r\n                        rounded\r\n                        dense\r\n                        class=\"blue-grey lighten-4\">\r\n                  <v-toolbar-title class=\"font-weight-medium\">{{title}}</v-toolbar-title> \r\n                </v-toolbar>\r\n              </v-col>\r\n              <v-col cols=\"1\">\r\n                <v-toolbar flat\r\n                        rounded\r\n                        dense\r\n                        class=\"blue-grey lighten-4\">\r\n                  <v-tooltip bottom color=\"primary\">\r\n                        <template v-slot:activator=\"{on, attrs}\">\r\n                          <v-btn icon\r\n                              color=\"primary\"\r\n                              v-bind=\"attrs\"\r\n                              v-on=\"on\"\r\n                              :disabled=\"false\"\r\n                              @click=\"onInsertItem\">\r\n                              <v-icon>mdi-file-plus</v-icon>\r\n                          </v-btn>\r\n                        </template>\r\n                        <span>Incluir</span>\r\n                  </v-tooltip>\r\n                  </v-toolbar>\r\n              </v-col>\r\n              <v-col cols=\"12\">\r\n                  <v-text-field :label=\"searchlabel\"\r\n                                single-line\r\n                                dense\r\n                                outlined\r\n                                prepend-inner-icon=\"mdi-magnify\"\r\n                                hide-details\r\n                                v-model=\"fieldSearch\"\r\n                                :value=\"fieldSearch = (fieldSearch== null) ? fieldSearch : fieldSearch.toUpperCase()\">\r\n\r\n                  </v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n            <!--ITEMS-->\r\n            <v-row dense>\r\n              <v-col>\r\n                <v-data-table no-data-text=\"Nenhum Registro Disponível\"\r\n                              no-results-text=\"Nenhum Registro Encontrado\"\r\n                              dense\r\n                              fixed-header\r\n                              height=\"352px\"\r\n                              class=\"elevation-3\"\r\n                              item-key=\"Id\"\r\n                              single-select\r\n                              hide-default-footer\r\n                              :headers=\"tableHeader\"\r\n                              :items=\"tableItems\"\r\n                              :items-per-page=\"10\">\r\n                  <template v-slot:item.actions=\"{item}\">\r\n                      <v-tooltip bottom color=\"primary\">\r\n                        <template v-slot:activator=\"{on, attrs}\">\r\n                          <v-icon small\r\n                              color=\"primary\"\r\n                              v-bind=\"attrs\"\r\n                              v-on=\"on\"\r\n                              class=\"mr-2\"\r\n                              :disabled=\"false\"\r\n                              @click=\"item\">\r\n                              mdi-pencil\r\n                          </v-icon>\r\n                        </template>\r\n                        <span>Alterar</span>\r\n                      </v-tooltip>\r\n                      <v-tooltip bottom color=\"primary\">\r\n                        <template v-slot:activator=\"{on, attrs}\">\r\n                          <v-icon small\r\n                              color=\"primary\"\r\n                              v-bind=\"attrs\"\r\n                              v-on=\"on\"\r\n                              class=\"mr-2\"\r\n                              :disabled=\"false\"\r\n                              @click=\"itemDelete = item\">\r\n                              mdi-delete\r\n                          </v-icon>\r\n                        </template>\r\n                        <span>Excluir</span>\r\n                      </v-tooltip>\r\n                  </template>\r\n                </v-data-table>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row dense justify=\"center\">\r\n              <v-pagination v-if=\"pageCount>0\"\r\n                    v-model=\"pageNumber\"\r\n                    circle\r\n                    prev-icon=\"mdi-menu-left\"\r\n                    next-icon=\"mdi-menu-right\"\r\n                    :length=\"pageCount\"></v-pagination>\r\n            </v-row>\r\n          </v-container>\r\n        </v-card>\r\n    </v-container>\r\n  </sidebar-layout-vue>\r\n</template>\r\n\r\n<script>\r\nimport SidebarLayoutVue from \"@/layouts/SidebarLayout.vue\";\r\n\r\nexport default {\r\n  name:\"FormList\",\r\n  components: {\r\n    SidebarLayoutVue\r\n  },\r\n  props: {\r\n      title: String,\r\n      routerInsert:\"\",\r\n      routerUpdate:\"\",\r\n      routerDelete:\"\",\r\n      tableHeader: [],\r\n      tableItems: [],\r\n      itemDelete:null,\r\n      fieldSearch: \"\"\r\n  },\r\n  data() {\r\n    return {\r\n      pageCount: 0,\r\n      pageNumber: 0,\r\n      searchlabel:\"Pesquisar\",\r\n    }\r\n  },\r\n  methods:{\r\n    onInsertItem(){\r\n      this.$router.push('/'+this.routerInsert);\r\n    },\r\n    onEditItem(item){\r\n      this.$router.push('/'+this.routerUpdate);\r\n    },\r\n    onDeleteItem(){\r\n      this.$router.push('/'+this.routerInsert);\r\n    }\r\n  }\r\n}\r\n\r\n</script>\r\n"]},"metadata":{},"sourceType":"module"}