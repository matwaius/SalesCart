Formatted = this.formatDate(this.date);\n    }\n\n  },\n  methods: {\n    formatDate(date) {\n      if (!date) return null;\n      const [year, month, day] = date.split('-');\n      return `${day}/${month}/${year}`;\n    },\n\n    parseDate(date) {\n      if (!date) return null;\n      const [day, month, year] = date.split('/');\n      return `${year}-${month.padStart(2, '0')}-${day.padStart(2, '0')}`;\n    },\n\n    Filter() {\n      reports.ReportNFDepartureDayByUF(this.date, this.date2).then(response => {\n        console.log(response.data);\n        this.Ufs = response.data.catch(error => console.log(error));\n      }).catch(error => console.log(error));\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8GA;AACA;AAEA;AAEA;EACAA,kBADA;EAEAC,WAFA;EAGAC;IACAC,QACA;MAAAC;MAAAC;MAAAC;IAAA,CADA,EAEA;MAAAF;MAAAC;MAAAC;IAAA,CAFA,EAGA;MAAAF;MAAAC;MAAAC;IAAA,CAHA,EAIA;MAAAF;MAAAC;MAAAC;IAAA,CAJA,EAKA;MAAAF;MAAAC;MAAAC;IAAA,CALA,EAMA;MAAAF;MAAAC;MAAAC;IAAA,CANA,EAOA;MAAAF;MAAAC;MAAAC;IAAA,CAPA,EAQA;MAAAF;MAAAC;MAAAC;IAAA,CARA,EASA;MAAAF;MAAAC;MAAAC;IAAA,CATA,CADA;IAYAC,+FAZA;IAaAC,gGAbA;IAcAC,uHAdA;IAeAC,wHAfA;IAgBAC,YAhBA;IAiBAC;EAjBA,EAHA;EAsBAC;IACAC,gBADA;IAEAC;EAFA,CAtBA;EA0BAC;IACAC;MACA;IACA;;EAHA,CA1BA;EA+BAC;IACAX;MACA;IACA,CAHA;;IAIAC;MACA;IACA;;EANA,CA/BA;EAuCAW;IACAC;MACA;MAEA;MACA;IACA,CANA;;IAOAC;MACA;MAEA;MACA;IACA,CAZA;;IAaAC;MACAC;QACAC;QACA,yBACAC,KADA,CACAC,2BADA;MAEA,CAJA,EAKAD,KALA,CAKAC,2BALA;IAMA;;EApBA;AAvCA","names":["name","show","data","items","text","icon","to","date","date2","dateFormatted","dateFormatted2","menu1","menu2","components","SidebarLayoutVue","FooterLayoutVue","computed","computedDateFormatted","watch","methods","formatDate","parseDate","Filter","reports","console","catch","error"],"sourceRoot":"src/views","sources":["Dashboard.vue"],"sourcesContent":["<template>\r\n  <app name=\"dashboard\">\r\n    <sidebar-layout-vue>\r\n\r\n  <v-container  class=\"fill-height\" fluid v-if=\"show\">\r\n    <v-row>\r\n      <v-col\r\n        cols